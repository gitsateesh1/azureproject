{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sateeshdf"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Azure_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_sql'"
		},
		"Snowflake1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sampleprep.dfs.core.windows.net/"
		},
		"Azure_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sateesh.database.windows.net"
		},
		"Azure_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sateesh"
		},
		"Azure_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sateesh"
		},
		"Snowflake1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TUTORIAL_DB"
		},
		"Snowflake1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "RCEPHRO-MK41907.snowflakecomputing.com"
		},
		"linkedService1_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'linkedService1'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sateeshdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"Azure_sql_password": {
						"value": "[parameters('Azure_sql_password')]"
					},
					"Snowflake1_password": {
						"value": "[parameters('Snowflake1_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Azure_sql_properties_typeProperties_server": {
						"value": "[parameters('Azure_sql_properties_typeProperties_server')]"
					},
					"Azure_sql_properties_typeProperties_database": {
						"value": "[parameters('Azure_sql_properties_typeProperties_database')]"
					},
					"Azure_sql_properties_typeProperties_userName": {
						"value": "[parameters('Azure_sql_properties_typeProperties_userName')]"
					},
					"Snowflake1_properties_typeProperties_database": {
						"value": "[parameters('Snowflake1_properties_typeProperties_database')]"
					},
					"Snowflake1_properties_typeProperties_host": {
						"value": "[parameters('Snowflake1_properties_typeProperties_host')]"
					},
					"linkedService1_sasUri": {
						"value": "[parameters('linkedService1_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}